//return sum
function int sum(int x, int y)
	int z = 0
	z = x + y
	end
	return z
end

function int sum(list arr)
	int res = 0
	for x in arr do
	  res = res + x
	end
	return res
end

main(int argc, list argv)
	int res =0
	list arr = [2,4,3,6,8,6]
	res = sum(arr)
	print(res)
end
