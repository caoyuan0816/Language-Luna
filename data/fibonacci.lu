function int fibonacci(int x)
  if (x==1) 
    return 1
  else 
    if (x==2)
      return 1
    else
      int y = fibonacci(x-1)
      int z = fibonacci(x-2)
      int k = y+z
      // can be changed to return expression in the future
      return k
    end
  end
end 

main()
  int res = fibonacci(3)
  print(res)
end
